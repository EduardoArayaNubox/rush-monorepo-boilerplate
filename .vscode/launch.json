			"name": "Run template oas mocha subset",
			"request": "launch",
			"type": "node",
			"program": "${workspaceFolder}/packages/oas/node_modules/mocha/bin/_mocha",
			"outputCapture": "std",
			"stopOnEntry": false,
			"args": [
				"--no-timeouts",
				"--grep",
				"${input:mochaGrep}"
			],
			"cwd": "${workspaceFolder}/packages/oas/",
			"runtimeExecutable": null,
			"env": {
				"USE_GRM": "true",
				"NODE_ENV": "test",
				"SITE_NAME": "test",
				"LOG_PRETTY": "true",
				"PUBSUB_EMULATOR_HOST": "localhost:8802",
