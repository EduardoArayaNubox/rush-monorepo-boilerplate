{
	"name": "@sixriver/template-service",
	"version": "0.0.0-development",
	"description": "Sixriver Template Service",
	"keywords": [
		"loopback-application",
		"loopback"
	],
	"main": "dist/src/index.js",
	"scripts": {
		"build": "npm run build:only",
		"rebuild": "npm-run-all clean build",
		"build:apidocs": "lb-apidocs",
		"build:only": "gulp compile --gulpfile '../../repo-tools/build-configs/gulpfile.js' --cwd './'",
		"build:watch": "gulp watch --gulpfile '../../repo-tools/build-configs/gulpfile.js' --cwd './'",
		"clean": "rm -rf dist* .rush/temp/package-deps*.json",
		"lint": "../../repo-tools/scripts/lint.sh",
		"test": "npm-run-all lint build test-ci",
		"test-ci": "../../repo-tools/scripts/test.sh",
		"unit": "../../repo-tools/scripts/test.sh --opts test/unit.opts",
		"test:no-cover": "NODE_ENV=test mocha ${MOCHA_OPTS}",
		"test:debug": "npm run test:no-cover -- --debug-brk",
		"coverage": "[ ! -d coverage/.nyc_output ] || NODE_ENV=test nyc report --reporter=text-lcov > coverage/lcov.info",
		"prestart": "rush build -t ${npm_package_name}",
		"start": "USE_GRM=true NODE_ENV=development SITE_NAME=development PUBSUB_EMULATOR_HOST=localhost:8802 PUBSUB_GCLOUD_PROJECT=development node .",
		"init-deploy-global": "../../ci_scripts/ci_tool.sh --rush_init_deploy",
		"build-publish-docker": "../../ci_scripts/ci_tool.sh --docker_rush_publish",
		"tag-docker": "../../ci_scripts/ci_tool.sh --docker_rush_add_version_tag",
		"htmlcov:gen": "NODE_ENV=test nyc report --reporter=html",
		"htmlcov:view": "npm run htmlcov:$(bash -c 'echo ${OSTYPE:0:5}')",
		"htmlcov:linux": "xdg-open ./coverage/.nyc_output/index.html",
		"htmlcov:darwi": "open ./coverage/.nyc_output/index.html",
		"htmlcov": "npm-run-all htmlcov:gen htmlcov:view",
		"db:migrations:create": "f(){ DATABASE_URL=postgres://localhost/template_service_development ./node_modules/.bin/db-migrate create $1 --migrations-dir=migrations/service --sql-file; }; f"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/6RiverSystems/rush-monorepo-boilerplate.git"
	},
	"publishConfig": {
		"registry": "https://sixriver.jfrog.io/sixriver/npm-local/"
	},
	"author": "",
	"license": "",
	"files": [
		"README.md",
		"index.js",
		"index.d.ts",
		"dist*/src",
		"dist*/index*",
		"src"
	],
	"dependencies": {
		"@loopback/boot": "2.2.0",
		"@loopback/context": "3.7.0",
		"@loopback/core": "2.5.0",
		"@loopback/metadata": "2.1.3",
		"@loopback/openapi-v3": "3.3.1",
		"@loopback/openapi-v3-types": "1.2.1",
		"@loopback/repository": "2.4.0",
		"@loopback/rest": "4.0.0",
		"@loopback/rest-explorer": "2.2.0",
		"@sixriver/config-schema": "1.46.1",
		"@sixriver/loopback4-support": "4.0.0",
		"@sixriver/service-directory": "4.0.0",
		"@sixriver/template-common": "0.0.0-development",
		"@sixriver/template-oas": "0.0.0-development",
		"@sixriver/typescript-support": "4.0.0",
		"ajv": "6.10.0",
		"axios": "0.21.1",
		"bunyan": "1.8.12",
		"db-migrate": "0.11.5",
		"db-migrate-pg": "0.4.0",
		"deep-eql": "3.0.1",
		"lodash": "4.17.19",
		"loopback-connector-postgresql": "3.6.0",
		"uuid": "3.4.0",
		"pino": "5.16.0",
		"pino-pretty": "3.2.1",
		"@sixriver/loopback4-metrics": "4.0.0",
		"prom-client": "12.0.0"
	},
	"devDependencies": {
		"@google-cloud/pubsub": "0.30.1",
		"@loopback/testlab": "3.1.3",
		"@types/bunyan": "1.8.4",
		"@types/chai": "4.2.5",
		"@types/chai-as-promised": "7.1.0",
		"@types/chai-string": "1.4.1",
		"@types/chance": "1.0.1",
		"@types/lodash": "4.14.159",
		"@types/mocha": "5.2.7",
		"@types/nock": "9.3.1",
		"@types/node": "12.12.54",
		"@types/sinon": "7.5.0",
		"@types/uuid": "3.4.6",
		"@typescript-eslint/eslint-plugin": "4.22.0",
		"@typescript-eslint/parser": "4.22.0",
		"bunyan-prettystream": "0.1.3",
		"chai": "4.2.0",
		"chai-as-promised": "7.1.1",
		"chai-string": "1.4.0",
		"chance": "1.0.13",
		"eslint": "7.24.0",
		"eslint-config-6river": "5.1.0-tsStrict.5",
		"eslint-config-google": "0.12.0",
		"eslint-plugin-6river": "1.0.6",
		"eslint-plugin-mocha": "8.1.0",
		"mocha": "5.1.1",
		"mocha-junit-reporter": "1.15.0",
		"mocha-lcov-reporter": "1.3.0",
		"nock": "10.0.6",
		"npm-run-all": "4.1.5",
		"nyc": "13.0.1",
		"sinon": "7.5.0",
		"source-map-support": "0.5.16",
		"typescript": "3.9.7",
		"gulp": "4.0.2",
		"gulp-sourcemaps": "2.6.5",
		"gulp-typescript": "5.0.1",
		"@6river/prettier-config": "1.0.37",
		"eslint-config-prettier": "8.2.0",
		"eslint-plugin-import": "2.22.1",
		"prettier": "2.2.1"
	}
}
